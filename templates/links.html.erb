<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
	<head>
		<meta http-equiv="content-type" content="text/html; charset=utf-8" />
		<title>Overview</title>		
		<link href="links.css" type="text/css" media="screen" rel="stylesheet" />
	</head>
  <body>
		<h2>Objects</h2>
		<ul class="objects">
    <% for object in @objects do %>
			<li class="object">
	    	<a href="<%= object.to_path %>" target="content"><%= object.to_s %></a>
	    </li>
    <% end %>
		</ul>
		<h2>Interfaces</h2>
		<ul class="interfaces">
    <% for interface in @interfaces do %>
			<li class="interface">
	    	<a href="<%= interface.to_path %>" target="content"><%= interface.to_s %></a>
	    </li>
    <% end %>
		</ul>
  </body>
</html>